{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "240",
            "template": "╭─"
          },
          {
            "background": "236",
            "foreground": "31",
            "properties": {
              "home_icon": "~",
              "style": "full",
              "gitdir_format": "<b><39>%s</></b>",
              "edge_format": "<b><39>%s</></b>"
            },
            "style": "plain",
            "template": " {{ .Path }} ",
            "type": "path"
          },
          {
            "style": "plain",
            "type": "text",
            "template": "{{ if .Segments.Git }}<244,236></>{{ else }}<236></>{{ end }}"
          },
          {
            "background": "236",
            "foreground": "250",
            "powerline_symbol": "",
            "properties": {
              "branch_icon": "\uf126 ",
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "plain",
            "templates": [
              " {{ if .Detached }}<246>@</><76>{{ trunc 8 .Commit.Sha }}</>{{ else }}<76>{{ .Ref }}</>{{ end }}",
              "{{ if gt .Behind 0 }} <76>⇣{{ .Behind }}</>{{ end }}",
              "{{ if gt .Ahead 0 }} <76>⇡{{ .Ahead }}</>{{ end }}",
              "{{ if gt .StashCount 0 }} <76>*{{ .StashCount }}</>{{ end }}",
              "{{ if .Merge }} <196>merge</>{{ end }}",
              "{{ if .Rebase }} <196>rebase {{ .Rebase.Current }}/{{ .Rebase.Total }}</>{{ end }}",
              "{{ if gt .Working.Unmerged 0 }} <196>~{{ .Working.Unmerged }}</>{{ end }}",
              "{{ if gt .Working.Untracked 0 }} <39>?{{ .Working.Untracked }}</>{{ end }}",
              "{{ if gt (add (add .Staging.Added .Staging.Modified) .Staging.Deleted) 0 }} <178>+{{ (add (add .Staging.Added .Staging.Modified) .Staging.Deleted) }}</>{{ end }}",
              "{{ if gt (add (add .Working.Added .Working.Modified) .Working.Deleted) 0 }} <178>!{{ (add (add .Working.Added .Working.Modified) .Working.Deleted) }}</>{{ end }}",
              " <236,transparent></>"
            ],
            "type": "git"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "background": "236",
            "foreground": "70",
            "foreground_templates": [
              "{{ if gt .Code 0 }}160{{ end }}"
            ],
            "properties": {
              "always_enabled": true
            },
            "style": "plain",
            "template": "<236,transparent></> {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}{{ end }} ",
            "type": "status"
          },
          {
            "type": "executiontime",
            "style": "plain",
            "background": "236",
            "foreground": "248",
            "template": "<244,236></> {{ .FormattedMs }} "
          },
          {
            "type": "text",
            "style": "plain",
            "background": "236",
            "foreground": "37",
            "template": "{{ if gt .Jobs 0 }}<244,236></> 󰒓 {{ end }}"
          },
          {
            "type": "text",
            "style": "plain",
            "background": "236",
            "foreground": "74",
            "template": "{{ if .Env.DIRENV_FILE }}<244,236></> 󱄅 {{ end }}"
          },
          {
            "background": "236",
            "foreground": "37",
            "properties": {
              "display_mode": "environment"
            },
            "style": "plain",
            "template": "<244,236></> {{ if .Error }}{{ .Error }}{{ else }}{{ .Venv }}{{ end }}  ",
            "type": "python"
          },
          {
            "background": "236",
            "foreground": "178",
            "style": "plain",
            "template": "<244,236></> \uf0ad ",
            "type": "root"
          },
          {
            "type": "session",
            "style": "plain",
            "background": "236",
            "foreground": "180",
            "template": "{{ if .SSHSession }}<244,236></> {{ .UserName }}@{{ .HostName }} {{ end }}"
          },
          {
            "type": "time",
            "style": "plain",
            "background": "236",
            "foreground": "66",
            "template": "<244,236></> {{ .CurrentDate | date .Format }} "
          },
          {
            "type": "text",
            "style": "plain",
            "foreground": "240",
            "template": "─╮ "
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "240",
            "template": "╰─"
          }
        ],
        "type": "prompt"
      },
      {
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "240",
            "template": "─╯"
          }
        ],
        "type": "rprompt"
      }
    ],
    "console_title_template": "{{ .Shell }} in {{ .Folder }}",
    "final_space": true,
    "version": 3
  }
